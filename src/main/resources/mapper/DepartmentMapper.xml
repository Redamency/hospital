        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shu.hospital.mapper.DepartmentMapper">

    <resultMap id="BaseResultMap" type="com.shu.hospital.domain.Department">
            <id property="deptNo" column="dept_no" jdbcType="INTEGER"/>
